{
	"manifest_version": 1,
	"id": "sqlexplorer",
	"name": "SQL Explorer",
	"author": "gr8b",
	"description": "Make SQL queries and export results to .csv files",
	"version": "1.0",
	"namespace": "SqlExplorer",
	"url": "https://github.com/gr8b/zabbix-module-sqlexplorer",
	"actions": {
		"sqlexplorer.form": {
			"class": "SqlForm",
			"view": "sql.form.view"
		},
		"sqlexplorer.queries": {
			"class": "StoredSql",
			"layout": "layout.json"
		},
		"sqlexplorer.csv": {
			"class": "SqlForm",
			"layout": "layout.csv"
		}
	}
}
